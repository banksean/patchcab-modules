{
    "name": "@banksean/patchcab-demo",
    "scope": "@banksean",
    "description": "demo package for patchcab modules",
    "version": "1.0.0",
    "publishConfig": {
        "access": "public"
    },
    "private": false,
    "author": "banksean (https://banksean.com)",
    "license": "MIT",
    "files": [
        "modules"
    ],
    "scripts": {
        "dev": "rollup -c -w",
        "start": "patchcab --rack testpatch.json",
        "build": "yarn build:lib && yarn build:web && yarn build:bin",
        "build:web": "rollup -c",
        "build:lib": "tsc --project ./config/lib.tsconfig.json && cpy './src/components/*.svelte' './lib/components/'",
        "build:bin": "tsc --project ./config/bin.tsconfig.json",
        "prepublish": "yarn build",
        "vercel-build": "npm run build && npm run build --prefix ../modules && cpy '../modules/public/modules.json' './public/' && cpy '../modules/public/modules' './public/modules/@patchcab-modules'",
        "postinstall": "touch ./.metadata_never_index"
    },
    "prettier": {
        "useTabs": false,
        "arrowParens": "always",
        "semi": true,
        "bracketSpacing": true,
        "singleQuote": true,
        "printWidth": 120,
        "svelteSortOrder": "options-scripts-styles-markup"
    },
    "dependencies": {
        "@banksean/patchcab-additiveosc": "../additiveosc",
        "@banksean/patchcab-cvsequencer": "../cvsequencer",
        "@banksean/patchcab-midiin": "../midiin",
        "@banksean/patchcab-polyrhythm": "../polyrhythm",
        "@patchcab/core": "^1.0.5",
        "@patchcab/modules": "^1.0.5",
        "rollup": "^2.45.2"
    },
    "devDependencies": {
        "lerna": "^4.0.0"
    },
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/banksean/patchcab-modules.git"
    },
    "bugs": {
        "url": "https://github.com/banksean/patchcab-modules/issues"
    },
    "homepage": "https://github.com/banksean/patchcab-modules#readme"
}